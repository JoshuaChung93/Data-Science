   "source": [
    "import cv2\n",
    "\n",
    "src = cv2.imread(\"plum.webp\", cv2.IMREAD_COLOR)\n",
    "dst = cv2.blur(src, (9, 9), anchor=(-1, -1), borderType=cv2.BORDER_DEFAULT) # 흐림 효과 함수 cv2.blur(입력 이미지, 커널 크기, 고정점, 테두리 외삽법)\n",
    "\n",
    "cv2.imshow(\"dst\", dst)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
