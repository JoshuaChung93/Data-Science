   "source": [
    "import cv2\n",
    "\n",
    "src = cv2.imread(\"geese.webp\", cv2.IMREAD_COLOR)\n",
    "\n",
    "gray = cv2.cvtColor(src, cv2.COLOR_BGR2GRAY)\n",
    "ret, dst = cv2.threshold(gray, 100, 255, cv2.THRESH_BINARY) # 이진화 함수, cv2.threshold(입력 이미지, 임곗값, 최댓값, 형식)\n",
    "\n",
    "cv2.imshow(\"dst\", dst)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
