   "source": [
    "import cv2\n",
    "\n",
    "src = cv2.imread(\"whitebutterfly.webp\", cv2.IMREAD_COLOR)\n",
    "dst = cv2.bitwise_not(src) # cv2.bitwise_not(src[입력 이미지], mask[마스크])\n",
    "\n",
    "cv2.imshow(\"src\", src)\n",
    "cv2.imshow(\"dst\", dst)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
